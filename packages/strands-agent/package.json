    "runtime": "node dist/runtime.js"
  },
  "dependencies": {
    "@strands-agents/sdk": "^0.1.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.145",
    "@types/node": "^22.0.0",
    "esbuild": "^0.24.0",
    "tsx": "^4.0.0",
    "typescript": "^5.6.0"
  }
}
